        "Fizetendő:?\\s*(\\d{1,3}(?:[., ]\\d{3})*(?:[.,]\\d{1,2})?)\\s*Ft",
        "(?:fizetési\\s+határidő).{1,50}(\\d{1,3}(?:[., ]\\d{3})*(?:[.,]\\d{1,2})?)\\s*Ft"
      ],
      "postProcessing": "removeSpaces"
    },
    {
      "fieldName": "dueDate",
      "label": "Fizetési határidő",
      "patterns": [
        "Fizetési\\s+határidő:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Esedékesség:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Befizetési\\s+határidő:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})"
      ]
    },
    {
      "fieldName": "billingDate",
      "label": "Számla kiállítás dátuma",
      "patterns": [
        "Dátum:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Kelt:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Kiállítás\\s+dátum:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Számla\\s+kelte:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})",
        "Teljesítés kelte:?\\s*(\\d{4}[.\\/-]\\d{1,2}[.\\/-]\\d{1,2})"
      ]
    },
    {
      "fieldName": "accountNumber",
      "label": "Ügyfél azonosító",
